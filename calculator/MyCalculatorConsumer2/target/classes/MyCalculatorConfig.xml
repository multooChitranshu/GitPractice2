<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="adding" class="com.nagalakshmi.demo.Sum">
	 <constructor-arg name="op1"  value="20"></constructor-arg>
		<constructor-arg name="op2"  value="40"></constructor-arg>
	</bean>

	<bean id="div" class="com.nagalakshmi.demo.Division">
	<property name="op1"  value="20"></property>
	<property name="op2"  value="10"></property>
	
	</bean>
	
	<bean id="sub" class="com.nagalakshmi.demo.Subtraction">
	<property name="op1"  value="20"></property>
	<property name="op2"  value="10"></property></bean>
	

	<bean id="multiply" class="com.nagalakshmi.demo.Multiplication">
		<constructor-arg name="op1"  value="20"></constructor-arg>
		<constructor-arg name="op2"  value="40"></constructor-arg>
	</bean>
</beans>
